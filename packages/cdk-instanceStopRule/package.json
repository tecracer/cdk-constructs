{
  "name": "cdk-instance-stop-rule",
  "description": "EC2 instance with cloudwatch rule to stop it",
  "keywords": [
    "cdk",
    "EC2",
    "cloudwatch rule"
  ],
  "version": "0.1.2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-package": "jsii",
    "package": "jsii-pacmak",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "tslint 'lib/**/*.ts{,x}'"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "tecracer.cdk",
        "module": "tecracer.ec2stoprule"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.22.0",
    "@types/jest": "^25.1.1",
    "@types/node": "10.17.5",
    "jest": "^24.9.0",
    "jsii": "^0.21.2",
    "jsii-pacmak": "^1.69.0",
    "ts-jest": "^24.3.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@aws-cdk/core": "^1.22.0",
    "@aws-cdk/aws-events": "^1.22.0",
    "@aws-cdk/aws-ec2": "^1.22.0",
    "@aws-cdk/aws-iam": "^1.22.0",
    "@aws-cdk/aws-sns": "^1.22.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.22.0",
    "@aws-cdk/aws-sqs": "^1.22.0"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.22.0",
    "@aws-cdk/aws-ec2": "^1.22.0",
    "@aws-cdk/aws-events": "^1.22.0",
    "@aws-cdk/aws-iam": "^1.22.0",
    "@aws-cdk/aws-sns": "^1.22.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.22.0",
    "@aws-cdk/aws-sqs": "^1.22.0"
  },
  "author": {
    "name": "Gernot Glawe"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:tecracer/cdk-constructs.git",
    "directory": "packages/@trc-cdk/instanceStopRule"
  }
}
